{
	"extends": "../tsconfig.json",
	"compilerOptions": {
		"types": [
			"webpack-env",
			"jest",
			"vuetify"
		],
		"paths": {
			"@/*": [
				"src/*"
			]
		}
	},
	"include": [
		"src/**/*.ts",
		"src/**/*.tsx",
		"src/**/*.vue",
		"tests/**/*.ts",
		"tests/**/*.tsx"
	],
	"exclude": ["node_modules", "dist", "ts-out"]
}